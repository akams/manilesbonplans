{
  "compilerOptions": {
    "paths": {
      "@Assets/*": ["./assets/*"],
      "@Atoms/*": ["./atoms/*"],
      "@Atoms": ["./atoms/index"],
      "@Components/*": ["./components/*"],
      "@Components": ["./components/index"],
      "@FirebaseConfig/*": ["./firebaseConfig/*"],
      "@Hoc/*": ["./hoc/*"],
      "@Hoc": ["./hoc/index"],
      "@Molecules/*": ["./molecules/*"],
      "@Molecules": ["./molecules/index"],
      "@Organisms/*": ["./organisms/*"],
      "@Organisms": ["./organisms/index"],
      "@Store": ["./store/index"],
      "@Store/*": ["./store/*"],
      "@Styles": ["./styles/index"],
      "@Styles/*": ["./styles/*"],
      "@Services": ["./styles/services"],
      "@Services/*": ["./services/*"],
      "@Types/*": ["./types/*"],
      "@Types": ["./types/index"],
      "@Utils/*": ["./utils/*"],
      "@Locale": ["./locale/index"],
      "@Locale/*": ["./locale/*"],
      "@Api": ["./pages/api/index"],
      "@Api/*": ["./pages/api/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
