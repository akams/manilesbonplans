{
  "name": "manilesbonplans",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "dev": "next dev -p 3005",
    "build": "next build",
    "build:export": "next build && next export",
    "start": "next start",
    "start:port": "next start -p 8080",
    "lint": "next lint"
  },
  "moduleNameMapper": {
    "^@Assets/(.*)$": "<rootDir>/assets/$1",
    "^@Atoms/(.*)$": "<rootDir>/atoms/$1",
    "^@Atoms$": "<rootDir>/atoms/index",
    "^@Components/(.*)$": "<rootDir>/components/$1",
    "^@Components$": "<rootDir>/components/index",
    "^@Hoc/(.*)$": "<rootDir>/hoc/$1",
    "^@Hoc$": "<rootDir>/hoc/index",
    "^@FirebaseConfig/(.*)$": "<rootDir>/firebaseConfig/$1",
    "^@Molecules/(.*)$": "<rootDir>/molecules/$1",
    "^@Molecules$": "<rootDir>/molecules/index",
    "^@Organisms/(.*)$": "<rootDir>/organisms/$1",
    "^@Organisms$": "<rootDir>/organisms/index",
    "^@Styles/(.*)$": "<rootDir>/styles/$1",
    "^@Styles$": "<rootDir>/styles/index",
    "^@Store/(.*)$": "<rootDir>/store/$1",
    "^@Store$": "<rootDir>/store/index",
    "^@Services/(.*)$": "<rootDir>/services/$1",
    "^@Services$": "<rootDir>/services/index",
    "^@Types/(.*)$": "<rootDir>/types/$1",
    "^@Types$": "<rootDir>/types/index",
    "^@Utils/(.*)$": "<rootDir>/utils/$1",
    "^@Locale/(.*)$": "<rootDir>/locale/$1",
    "^@Locale$": "<rootDir>/locale/index",
    "^@Api/(.*)$": "<rootDir>/pages/api/$1",
    "^@Api$": "<rootDir>/pages/api/index"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.5",
    "@reduxjs/toolkit": "^1.8.0",
    "axios": "^0.27.2",
    "firebase": "^9.9.4",
    "firebase-admin": "^11.0.1",
    "firebase-functions": "^3.23.0",
    "next": "12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.35.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intl": "^6.1.1",
    "react-outside-click-handler": "^1.3.0",
    "react-query": "^3.39.2",
    "react-redux": "^7.2.6",
    "react-select": "^5.2.2",
    "sharp": "^0.31.0",
    "styled-components": "^5.3.5",
    "uniqid": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "18.7.16",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@types/react-outside-click-handler": "^1.3.1",
    "@types/styled-components": "^5.1.26",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  }
}
